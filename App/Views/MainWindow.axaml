<reactiveui:ReactiveWindow
    x:TypeArguments="vms:MainWindowViewModel"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:reactiveui="http://reactiveui.net"
    xmlns:vms="clr-namespace:App.ViewModels"
    xmlns:controls="clr-namespace:App.Controls"
    xmlns:models="clr-namespace:App.Models"
    mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
    x:Class="App.Views.MainWindow">
    
    <controls:BrowsingTabControl x:Name="MainTabControl" TabStripPlacement="Top">
        <controls:BrowsingTabItem>
            <models:HeaderModelBase Title="Title model base"/>
        </controls:BrowsingTabItem>
    </controls:BrowsingTabControl>

</reactiveui:ReactiveWindow>
