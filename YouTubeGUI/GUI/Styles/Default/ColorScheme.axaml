<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Main colors -->
    <Color x:Key="MainBgColor">#252525</Color>
    <Color x:Key="MainFgColor">#FFFFFF</Color>
    
    <!-- Main brushes -->
    <SolidColorBrush x:Key="MainBgBrush" Color="{DynamicResource MainBgColor}"/>
    <SolidColorBrush x:Key="MainFgBrush" Color="{DynamicResource MainFgColor}"/>
</ResourceDictionary>
